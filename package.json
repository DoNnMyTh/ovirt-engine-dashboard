{
  "name": "ovirt-engine-dashboard",
  "version": "1.2.0",
  "description": "oVirt UI plugin that gives overview of the virtualized environment and its specific objects.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://gerrit.ovirt.org/ovirt-engine-dashboard"
  },
  "scripts": {
    "prebuild": "yarn run test",
    "build": "NODE_ENV=production webpack -p --bail",
    "dev": "webpack -d --progress --watch",
    "intl:extract": "EXTRACT_MESSAGES=1 webpack -d && rip json2pot 'extra/messages/**/*.json' -o 'extra/messages/dashboard-messages.pot'",
    "intl:push-all": "cd zanata && zanata-cli push --push-type source --locales de,es,fr,it,ja,ko,pt-BR,zh-CN",
    "intl:pull-all": "cd zanata && zanata-cli pull --pull-type trans --locales de,es,fr,it,ja,ko,pt-BR,zh-CN",
    "intl:apply": "rip po2json 'extra/messages/po/dashboard-messages.*.po' -m 'extra/messages/**/*.json' -o 'intl/translations.json'",
    "intl:normalize": "zanata/normalize-translations.js",
    "pretest": "eslint src --ext js,jsx",
    "test": "NODE_ENV=test karma start --single-run --no-auto-watch",
    "test:watch": "NODE_ENV=test karma start --no-single-run --auto-watch"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "=3.3.7",
    "c3": "^0.4.10",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "clean-webpack-plugin": "^0.1.8",
    "copy-webpack-plugin": "^2.1.1",
    "css-element-queries": "^0.4.0",
    "css-loader": "^0.23.1",
    "d3": "^3.5.16",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.6.5",
    "intl": "^1.2.4",
    "intl-messageformat": "^1.3.0",
    "intl-messageformat-parser": "^1.2.0",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.17.4",
    "mocha": "^2.5.3",
    "patternfly": "=3.30.0",
    "phantomjs-prebuilt": "^2.1.7",
    "prop-types": "^15.5.10",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-height-matching-group": "^1.0.8",
    "react-intl": "^2.1.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^2.1.3",
    "json-stable-stringify": "^1.0.1",
    "react-intl-po": "^1.0.5",
    "webpack-dev-server": "^1.14.1"
  }
}
